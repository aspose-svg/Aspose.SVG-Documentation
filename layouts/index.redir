#  Nginx redirects. baseURL = {{ .Site.BaseURL }}, basePath = {{ .Site.Params.basePath }}
{{ $basePath := .Site.Params.basePath -}}
{{ if eq $basePath "/" -}}
{{ $basePath = "" -}}
{{ end -}}
{{ range $p := .Site.Pages -}}
{{   range  .Aliases -}}
{{     if ne .  $p.RelPermalink -}}
  {{- $basePath -}} {{ . |   printf "%-60s" }}	{{ $p.RelPermalink   -}};
{{     end -}}
{{   end -}}
{{ end -}}